    "pretty": "prettier --write --no-semi --single-quote --trailing-comma es5",
    "test": "jest"
  },
  "dependencies": {
    "prop-types-docs": "0.5.0",
    "stylegator": "0.20.2"
  },
  "devDependencies": {
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.2"
  }
}
