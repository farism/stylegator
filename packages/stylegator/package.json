{
  "name": "stylegator",
  "version": "0.18.0",
  "description": "styleguide static generator for html and js",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farism/stylegator.git"
  },
  "keywords": [
    "cli",
    "styleguide",
    "static generator"
  ],
  "author": "Faris Mustafa <farismmk@gmail.com>",
  "license": "MIT",
  "bin": {
    "stylegator": "./bin/cli.js"
  },
  "scripts": {
    "build": "yarn clean && babel src -d lib",
    "clean": "rimraf lib",
    "dev": "babel src -d lib --watch",
    "lint-staged": "lint-staged",
    "pretty": "prettier --write --no-semi --single-quote --trailing-comma es5",
    "test": "jest"
  },
  "dependencies": {
    "@stylegator/app": "^0.18.0",
    "@stylegator/cli": "^0.18.0",
    "@stylegator/core": "^0.18.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/preset-env": "7.3.1",
    "rimraf": "2.6.3"
  }
}
