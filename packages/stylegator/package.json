{
  "name": "@stylegator/stylegator",
  "version": "0.5.2",
  "description": "styleguide static generator for html and js",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farism/stylegator.git"
  },
  "keywords": [
    "cli"
  ],
  "author": "Faris Mustafa <farismmk@gmail.com>",
  "license": "MIT",
  "bin": {
    "stylegator": "./bin/cli.js"
  },
  "scripts": {
    "build": "babel src -d lib --copy-files",
    "dev": "babel src -d lib --copy-files --watch",
    "lint-staged": "lint-staged",
    "pretty": "prettier --write --no-semi --single-quote --trailing-comma es5",
    "test": "jest"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "copy-webpack-plugin": "4.3.1",
    "fuzzy": "^0.1.3",
    "highlight.js": "9.12.0",
    "html-webpack-plugin": "2.30.1",
    "marked": "0.3.12",
    "ramda": "^0.25.0",
    "raw-loader": "0.5.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-live": "1.9.2",
    "react-router-dom": "4.2.2",
    "recompose": "^0.26.0",
    "slugify": "1.2.9",
    "webpack": "3.10.0",
    "webpack-blocks": "1.0.0-rc.2",
    "webpack-cleanup-plugin": "0.5.1",
    "webpack-dev-server": "2.11.1",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-sources": "^1.1.0",
    "webpack-stats-plugin": "^0.1.5"
  },
  "devDependencies": {
    "husky": "0.14.3",
    "lint-staged": "6.1.0",
    "prettier": "1.10.2"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=6.0.0"
  }
}
