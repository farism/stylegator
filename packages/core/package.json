{
  "name": "@stylegator/core",
  "version": "0.20.0",
  "description": "styleguide static generator for html and js",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farism/stylegator.git"
  },
  "keywords": [
    "cli",
    "styleguide",
    "static generator"
  ],
  "author": "Faris Mustafa <farismmk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && babel src -d lib",
    "clean": "rimraf lib",
    "dev": "babel src -d lib --watch",
    "lint-staged": "lint-staged",
    "pretty": "prettier --write --no-semi --single-quote --trailing-comma es5",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/plugin-proposal-object-rest-spread": "7.7.4",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.6",
    "@babel/preset-react": "7.7.4",
    "babel-loader": "8.0.6",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.3.2",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.13.0",
    "raw-loader": "4.0.0",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.1",
    "url-loader": "3.0.0",
    "webpack": "4.41.3",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.2.2",
    "webpack-sources": "1.4.3",
    "webpack-stylish": "0.1.8"
  },
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "conventional-changelog-cli": "2.0.31",
    "conventional-recommended-bump": "6.0.5",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.0"
  }
}
