{
  "name": "@stylegator/core",
  "version": "0.19.0",
  "description": "styleguide static generator for html and js",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farism/stylegator.git"
  },
  "keywords": [
    "cli",
    "styleguide",
    "static generator"
  ],
  "author": "Faris Mustafa <farismmk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && babel src -d lib",
    "clean": "rimraf lib",
    "dev": "babel src -d lib --watch",
    "lint-staged": "lint-staged",
    "pretty": "prettier --write --no-semi --single-quote --trailing-comma es5",
    "test": "jest"
  },
  "dependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.4",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.9.3",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "3.0.0",
    "sass-loader": "7.3.1",
    "style-loader": "0.23.0",
    "url-loader": "2.3.0",
    "webpack": "4.20.2",
    "webpack-dev-server": "3.1.11",
    "webpack-merge": "4.1.4",
    "webpack-sources": "1.4.3",
    "webpack-stylish": "0.1.8"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "husky": "3.0.8",
    "lint-staged": "7.3.0",
    "prettier": "1.14.3",
    "rimraf": "2.7.1"
  }
}
