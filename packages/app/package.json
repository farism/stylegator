{
  "name": "@stylegator/app",
  "version": "0.18.1-rc.17",
  "description": "styleguide static generator for html and js",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farism/stylegator.git"
  },
  "keywords": [
    "cli",
    "styleguide",
    "static generator"
  ],
  "author": "Faris Mustafa <farismmk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && babel src -d lib --copy-files",
    "clean": "rimraf lib",
    "dev": "babel src -d lib --copy-files --watch",
    "lint-staged": "lint-staged",
    "pretty": "prettier --write --no-semi --single-quote --trailing-comma es5",
    "test": "jest"
  },
  "dependencies": {
    "fuzzy": "0.1.3",
    "highlight.js": "9.13.1",
    "marked": "0.5.2",
    "ramda": "0.25.0",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-live": "1.12.0",
    "react-router-dom": "4.3.1",
    "recompose": "0.30.0",
    "slugify": "1.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "husky": "1.1.2",
    "lint-staged": "8.1.0",
    "prettier": "1.14.3",
    "rimraf": "2.6.2"
  }
}
