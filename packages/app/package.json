{
  "name": "@stylegator/app",
  "version": "0.12.0",
  "description": "styleguide static generator for html and js",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farism/stylegator.git"
  },
  "keywords": [
    "cli",
    "styleguide",
    "static generator"
  ],
  "author": "Faris Mustafa <farismmk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && babel src -d lib --copy-files",
    "clean": "rimraf lib",
    "dev": "babel src -d lib --copy-files --watch",
    "lint-staged": "lint-staged",
    "pretty": "prettier --write --no-semi --single-quote --trailing-comma es5",
    "test": "jest"
  },
  "dependencies": {
    "fuzzy": "^0.1.3",
    "highlight.js": "9.12.0",
    "marked": "0.3.19",
    "ramda": "^0.25.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-live": "1.10.1",
    "react-router-dom": "4.2.2",
    "recompose": "^0.27.0",
    "slugify": "1.2.9"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "husky": "0.14.3",
    "lint-staged": "7.1.0",
    "prettier": "1.12.1",
    "rimraf": "2.6.2"
  }
}
