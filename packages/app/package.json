{
  "name": "@stylegator/app",
  "version": "0.18.1-rc.43",
  "description": "styleguide static generator for html and js",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farism/stylegator.git"
  },
  "keywords": [
    "cli",
    "styleguide",
    "static generator"
  ],
  "author": "Faris Mustafa <farismmk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && babel src -d lib --copy-files",
    "clean": "rimraf lib",
    "dev": "babel src -d lib --copy-files --watch",
    "lint-staged": "lint-staged",
    "pretty": "prettier --write --no-semi --single-quote --trailing-comma es5",
    "test": "jest"
  },
  "dependencies": {
    "fuzzy": "0.1.3",
    "highlight.js": "9.14.2",
    "marked": "0.6.1",
    "ramda": "0.26.1",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "react-live": "1.12.0",
    "react-router-dom": "4.3.1",
    "recompose": "0.30.0",
    "slugify": "1.3.4"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.3.3",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.4",
    "prettier": "1.16.4",
    "rimraf": "2.6.3"
  }
}
