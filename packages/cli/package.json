{
  "name": "@stylegator/cli",
  "version": "0.20.0",
  "description": "Stylegator CLI tool",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farism/stylegator.git"
  },
  "keywords": [
    "cli",
    "styleguide",
    "static generator"
  ],
  "author": "Faris Mustafa <farismmk@gmail.com>",
  "license": "MIT",
  "bin": {
    "stylegator": "./bin/cli.js"
  },
  "scripts": {
    "build": "yarn clean && babel src -d lib",
    "clean": "rimraf lib",
    "dev": "babel src -d lib --watch",
    "lint-staged": "lint-staged",
    "pretty": "prettier --write --no-semi --single-quote --trailing-comma es5",
    "test": "jest"
  },
  "dependencies": {
    "@stylegator/core": "0.20.0",
    "conventional-changelog-cli": "2.0.31",
    "conventional-recommended-bump": "6.0.5",
    "find-up": "4.1.0",
    "yargs": "15.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.7.5",
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.0"
  }
}
