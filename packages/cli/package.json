    "lint-staged": "lint-staged",
    "pretty": "prettier --write --no-semi --single-quote --trailing-comma es5",
    "test": "jest"
  },
  "dependencies": {
    "@stylegator/core": "^0.16.2",
    "find-up": "3.0.0",
    "yargs": "12.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.1",
    "@babel/cli": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "prettier": "1.14.2",
    "rimraf": "2.6.2"
  }
}
