{
  "private": true,
  "devDependencies": {
    "execa-pro": "1.0.2",
    "husky": "0.14.3",
    "lerna": "2.7.1",
    "lerna-git-flow-deploy": "0.5.1",
    "semver": "5.5.0",
    "tmp": "0.0.33"
  },
  "scripts": {
    "build": "lerna run --no-parallel build",
    "dev": "lerna run dev",
    "precommit": "lerna run lint-staged",
    "deploy:next": "lerna-deploy --type=next",
    "deploy:stable": "lerna-deploy --type=next",
    "deploy:ghpages": "node scripts/deploy_gh_pages",
    "pretty": "lerna run pretty",
    "test": "lerna run test"
  },
  "workspaces": [
    "packages/*"
  ]
}
